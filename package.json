{
  "name": "angular2-esri-example",
  "version": "0.1.1",
  "description": "Example app using the ArcGIS API for JavaScript v3 in an Angular2 app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomwayson/angular2-esri-example.git"
  },
  "scripts": {
    "test": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "postinstall": "typings install",
    "tsc": "tsc",
    "typings": "typings"
  },
  "keywords": [
    "Angular2",
    "Esri"
  ],
  "author": "Tom Wayson <tom@tomwayson.com>",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "esri-system-js": "0.0.3",
    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
